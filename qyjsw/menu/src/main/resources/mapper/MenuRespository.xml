<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.htqyjsw1.repository.MenuRepository">
    <select id="count" resultType="int">
        select count(*) from t_menu
    </select>
    <select id="findAll" resultType="com.htqyjsw1.entity.Menu">
		select * from t_menu;
	</select>
    <select id="findById" resultType="com.htqyjsw1.entity.Menu">
		select * from t_menu where menu_id = #{menu_id};
	</select>

</mapper>